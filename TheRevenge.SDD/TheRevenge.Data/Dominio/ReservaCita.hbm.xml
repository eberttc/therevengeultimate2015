<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2"
                   assembly="TheRevenge.Data"
                   namespace="TheRevenge.Data.Dominio"
                   default-lazy="false">
    <class name="ReservaCita" table="RESERVA_CITA">
        <id name="IdReserva" column="ID_RESERVA">
            <generator class="identity"></generator>
        </id>
        <property name="FechaReservaCita" column="FECHA_RESERVA"></property>
        <property name="Especialidad" column="ID_ESPECIALIDAD"></property>
        <property name="Medico" column="ID_MEDICO"></property>
        <property name="Paciente" column="ID_PACIENTE"></property>
        <property name="Horario" column="ID_HORARIO"></property>
        <property name="Estado" column="ID_ESTADO"></property>
        
    </class>
</hibernate-mapping>